get_modules() {
        if [ "$DRONE_REPO_NAME" == "coog" ]
        then
                cd /workspace/drone/coog \
                && git diff --name-only $(git merge-base origin/"$DRONE_BRANCH" "$DRONE_COMMIT_SHA") "$DRONE_COMMIT_SHA" \
                | grep "^modules" \
                | cut -d "/" -f 2 \
                | sort \
                | uniq
        else
                ls -1 /workspace/drone/customers/modules
        fi
}
